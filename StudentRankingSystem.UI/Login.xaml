<Window x:Class="StudentRankingSystem.UI.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentRankingSystem.UI"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Button x:Name="buttonLogin" Content="Login" HorizontalAlignment="Left" Margin="89,106,0,0" VerticalAlignment="Top" Width="75" Click="buttonLogin_Click"/>
        <TextBox x:Name="textBoxLogin" HorizontalAlignment="Left" Height="23" Margin="89,50,0,0" TextWrapping="Wrap" Text="admin" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="textBoxPassword" HorizontalAlignment="Left" Height="23" Margin="89,78,0,0" TextWrapping="Wrap" Text="123456" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
